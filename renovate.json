{
  "extends": [
    "config:base"
  ],
  "labels": ["dependencies"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "matchPackagePatterns": ["io.camunda.connector"],
      "matchManagers": ["maven"],
      "enabled": false
    }
  ],
  "baseBranches": ["main", "release/8.3", "release/8.4"]
}
