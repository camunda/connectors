    <bpmn:sequenceFlow id="Flow_0ljlich" name="Execute agent" sourceRef="Gateway_0d7bxjf" targetRef="Gateway_1onk6us">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">=action = "executeAgent"</bpmn:conditionExpression>
