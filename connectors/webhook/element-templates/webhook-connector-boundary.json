    "feel" : "required",
    "group" : "authentication",
    "binding" : {
      "name" : "inbound.hmacScopes",
      "type" : "zeebe:property"
    },
    "condition" : {
      "property" : "inbound.shouldValidateHmac",
      "equals" : "enabled",
      "type" : "simple"
    },
    "type" : "String"
  }, {
    "id" : "inbound.auth.type",
    "label" : "Authorization type",
    "description" : "Choose the authorization type",
