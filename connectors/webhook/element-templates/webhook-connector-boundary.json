    "group" : "authorization",
    "binding" : {
      "name" : "inbound.auth.type",
      "type" : "zeebe:property"
    },
    "type" : "Dropdown",
    "choices" : [ {
      "name" : "None",
      "value" : "NONE"
    }, {
      "name" : "Basic",
      "value" : "BASIC"
