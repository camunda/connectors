  "version" : 7,
  "category" : {
    "id" : "connectors",
    "name" : "Connectors"
  },
  "appliesTo" : [ "bpmn:StartEvent" ],
  "elementType" : {
    "value" : "bpmn:StartEvent"
  },
  "groups" : [ {
    "id" : "endpoint",
    "label" : "Webhook configuration"
  }, {
    "id" : "activation",
    "label" : "Activation"
  }, {
    "id" : "deduplication",
    "label" : "Deduplication",
