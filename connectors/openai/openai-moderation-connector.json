{
    "$schema": "https://unpkg.com/@camunda/zeebe-element-templates-json-schema/resources/schema.json",
    "name": "OpenAI Moderation Connector",
    "id": "089876d4-ada1-4181-b80e-ffdd46c7c9dc",
    "description": "Use openAI moderation to classify text content",
    "icon": {
      "contents": "data:image/png;base64,"
    },
    "documentationRef": "https://beta.openai.com/docs/guides/moderation/overview",
    "category": {
      "id": "connectors",
      "name": "Connectors"
    },
    "appliesTo": [
      "bpmn:Task"
    ],
    "elementType": {
      "value": "bpmn:ServiceTask"
    },
    "groups": [
      {
        "id": "authentication",
        "label": "Authentication"
      },
      {
        "id": "content",
        "label": "Input"
      },
      {
        "id": "output",
        "label": "Response Mapping"
      }
    ],
    "properties": [
      {
        "type": "Hidden",
        "value": "io.camunda:http-json:1",
        "binding": {
          "type": "zeebe:taskDefinition:type"
        }
      },
      {
        "label": "Type",
        "id": "authenticationType",
        "value": "bearer",
        "type": "Hidden",
        "binding": {
          "type": "zeebe:input",
          "name": "authentication.type"
        }
      },
      {
        "id": "method",
        "type": "Hidden",
        "value": "post",
        "binding": {
          "type": "zeebe:input",
          "name": "method"
        }
      },
      {
        "type": "Hidden",
        "value": "https://api.openai.com/v1/moderations",
        "binding": {
          "type": "zeebe:input",
          "name": "url"
        }
      },
      {
        "label": "HTTP Headers",
        "type": "Hidden",
        "value": "= {Content-Type: \"application/json\"}",
        "binding": {
          "type": "zeebe:input",
          "name": "headers"
        }
      },
      {
        "label": "openAI API Key",
        "group": "authentication",
        "description": "Your openAI API Key stored as secret",
        "type": "String",
        "binding": {
          "type": "zeebe:input",
          "name": "authentication.token"
        },
        "constraints": {
          "notEmpty": true
        }
      },
      {
        "label": "Evaluation input",
        "description": "Provide the text to analyze",
        "group": "content",
        "type": "Text",
        "feel": "required",
        "value": "{\"input\": \"Sample text goes here\"}",
        "binding": {
          "type": "zeebe:input",
          "name": "body"
        }
      },
      {
        "label": "Result Expression",
        "description": "Expression to map the response into process variables",
        "group": "output",
        "type": "Text",
        "feel": "required",
        "value": "={\r flagged: response.body.results[1].flagged,\r hate: response.body.results[1].category_scores.hate,\r sexual: response.body.results[1].category_scores.sexual,\r violence: response.body.results[1].category_scores.violence\r}",
        "binding": {
          "type": "zeebe:taskHeader",
          "key": "resultExpression"
        }
      }
    ]
  }