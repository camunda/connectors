    "binding" : {
      "name" : "deduplicationMode",
      "type" : "zeebe:property"
    },
    "condition" : {
      "property" : "deduplicationId",
      "isActive" : true,
      "type" : "simple"
    },
    "type" : "Hidden"
  }, {
    "id" : "deduplicationModeAuto",
    "value" : "AUTO",
    "group" : "deduplication",
    "binding" : {
      "name" : "deduplicationMode",
      "type" : "zeebe:property"
    },
    "condition" : {
      "property" : "deduplicationId",
      "isActive" : false,
      "type" : "simple"
    },
    "type" : "Hidden"
  }, {
    "id" : "resultVariable",
    "label" : "Result variable",
    "description" : "Name of variable to store the response in",
