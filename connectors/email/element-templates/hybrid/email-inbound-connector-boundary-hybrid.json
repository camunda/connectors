    "binding" : {
      "name" : "resultVariable",
      "type" : "zeebe:property"
    },
    "type" : "String"
  }, {
    "id" : "resultExpression",
    "label" : "Result expression",
    "description" : "Expression to map the response into process variables",
    "feel" : "required",
    "group" : "output",
    "binding" : {
      "name" : "resultExpression",
