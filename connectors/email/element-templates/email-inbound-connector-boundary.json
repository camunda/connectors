    "condition" : {
      "allMatch" : [ {
        "property" : "data.pollingConfig.unseenHandlingStrategy",
        "equals" : "MOVE",
        "type" : "simple"
      }, {
        "property" : "data.pollingConfigDiscriminator",
        "equals" : "unseenPollingConfig",
        "type" : "simple"
      } ]
    },
