    "constraints" : {
      "notEmpty" : true
    },
    "group" : "authentication",
    "binding" : {
      "name" : "authentication.password",
      "type" : "zeebe:property"
    },
    "condition" : {
      "property" : "authentication.type",
      "equals" : "simple",
      "type" : "simple"
    },
